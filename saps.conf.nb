#Main programs options
Saps:
  #Collect
  DirSet: ~/Dissertation/sets

  #Plot configuration
  DirPlot: ~/Dissertation/plots
  Plot2X: True
  Plot2EpsLatex: False
  Plot2EpsLatexShow: True
  PdfViewer: acroread

  #Debug
  DebugAnalyse: True
  DebugCollect: False
  DebugRestructure: False
  DebugPlot: True

  #Msg configuration
  ShowNotice: False
  ShowWarning: True
  RoundDigits: 13

#Module selection
Simulate: FilesystemCluster.py
Collect: FilesystemITPP.py

#Module configuration
FilesystemITPP:
  DirResults: ~/Dissertation/results
  StopOnDefectResultFiles: False
  StopOnIncompleteResultFiles: False

FilesystemCluster:
  DirJob: ~/Dissertation/cluster/jobs/waiting

PbsCluster:
  DirJob: ~/Dissertation/cluster/jobs/waiting
  DirLog: ~/Dissertation/cluster/logs
